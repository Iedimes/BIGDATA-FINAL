    <sql>-- Configurar el esquema de preparación
SET search_path TO final;

-- Configurar el idioma a español
SET lc_time = 'es_ES';

/* PASO 1 - Transformación y Limpieza de Datos
 * Crear tablas temporales y realizar limpieza y transformación.
 */
DROP TABLE IF EXISTS tmp_vacunados;

CREATE TEMPORARY TABLE tmp_vacunados AS
SELECT DISTINCT
    nombre,
    apellido,
    establecimiento,
    fecha_aplicacion,
    cedula,
    dosis,
    descripcion_vacuna,
    actualizado_al
FROM vacunados_temporal
WHERE establecimiento IS NOT NULL
  AND descripcion_vacuna IS NOT NULL
  AND nombre NOT LIKE '%.%';

/* PASO 2 - Crear tablas secundarias
 * Crear tablas de referencia y principal para datos limpios.
 */

-- Primero, eliminar vacunados_oficial si existe, debido a las dependencias.
DROP TABLE IF EXISTS vacunados_oficial CASCADE;

-- Ahora eliminar las tablas de referencia
DROP TABLE IF EXISTS establecimientos CASCADE;
DROP TABLE IF EXISTS vacunas CASCADE;

-- Crear las tablas nuevamente
CREATE TABLE establecimientos (
    id SERIAL PRIMARY KEY,
    descripcion TEXT UNIQUE
);

CREATE TABLE vacunas (
    id SERIAL PRIMARY KEY,
    descripcion TEXT UNIQUE
);

DROP TABLE IF EXISTS vacunados_datos_limpios;

CREATE TABLE vacunados_datos_limpios (
    nombre TEXT,
    apellido TEXT,
    establecimiento TEXT,
    fecha_aplicacion DATE,
    cedula VARCHAR(25),
    dosis INTEGER,
    descripcion_vacuna TEXT,
    actualizado_al DATE
);

/* PASO 3 - Carga de Datos
 * Insertar datos en las tablas de referencia y la tabla principal con datos limpios.
 */

-- Insertar establecimientos únicos
INSERT INTO establecimientos (descripcion)
SELECT DISTINCT establecimiento
FROM tmp_vacunados
ON CONFLICT (descripcion) DO NOTHING;

-- Insertar vacunas únicas con nombres normalizados
INSERT INTO vacunas (descripcion)
SELECT DISTINCT
    CASE 
        WHEN descripcion_vacuna ILIKE '%sinopharm%' THEN 'Sinopharm'
        WHEN descripcion_vacuna ILIKE '%pfizer%' THEN 'Pfizer'
        WHEN descripcion_vacuna ILIKE '%sputnik%' THEN 'Sputnik V'
        WHEN descripcion_vacuna ILIKE '%moderna%' THEN 'Moderna'
        WHEN descripcion_vacuna ILIKE '%astrazeneca%' THEN 'AstraZeneca'
        WHEN descripcion_vacuna ILIKE '%coronavac%' THEN 'Coronavac'
        WHEN descripcion_vacuna ILIKE '%hayat%' THEN 'Hayat Vax'
        WHEN descripcion_vacuna ILIKE '%covaxin%' THEN 'Covaxin'
        ELSE INITCAP(TRIM(descripcion_vacuna))
    END AS descripcion
FROM tmp_vacunados
ON CONFLICT (descripcion) DO NOTHING;

-- Poblar la tabla de vacunados_datos_limpios con la descripción de vacuna normalizada
INSERT INTO vacunados_datos_limpios (
    nombre, apellido, establecimiento, fecha_aplicacion, cedula, dosis, descripcion_vacuna, actualizado_al)
SELECT 
    v.nombre,
    v.apellido,
    v.establecimiento,
    v.fecha_aplicacion,
    v.cedula,
    v.dosis,
    CASE 
        WHEN v.descripcion_vacuna ILIKE '%sinopharm%' THEN 'Sinopharm'
        WHEN v.descripcion_vacuna ILIKE '%pfizer%' THEN 'Pfizer'
        WHEN v.descripcion_vacuna ILIKE '%sputnik%' THEN 'Sputnik V'
        WHEN v.descripcion_vacuna ILIKE '%moderna%' THEN 'Moderna'
        WHEN v.descripcion_vacuna ILIKE '%astrazeneca%' THEN 'AstraZeneca'
        WHEN v.descripcion_vacuna ILIKE '%coronavac%' THEN 'Coronavac'
        WHEN v.descripcion_vacuna ILIKE '%hayat%' THEN 'Hayat Vax'
        WHEN v.descripcion_vacuna ILIKE '%covaxin%' THEN 'Covaxin'
        ELSE INITCAP(TRIM(v.descripcion_vacuna))
    END AS descripcion_vacuna,
    v.actualizado_al
FROM tmp_vacunados v;

</sql>
